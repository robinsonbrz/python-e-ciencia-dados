[flake8]
ignore = W503, I100, I101, E203, E731
exclude =
    .tox,
    .git,
    __pycache__,
    build,
    dist,
    *.pyc,
    *.egg-info,
    .cache,
    .eggs,
    *migrations*
    *static*
    *venv*
    .pytest_env
max-complexity = 15
max-line-length = 120
import-order-style = google
application-import-names = flake8